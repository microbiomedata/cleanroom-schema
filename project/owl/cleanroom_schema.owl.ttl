<https://w3id.org/microbiomedata/cleanroom-schema> a owl:Ontology ;
    rdfs:label "cleanroom-schema" ;
    IAO:0000700 cleanroom_schema:NamedThing,
        cleanroom_schema:PersonCollection ;
    dcterms:license "MIT" ;
    dcterms:title "cleanroom-schema" ;
    rdfs:seeAlso "https://microbiomedata.github.io/cleanroom-schema" ;
